{
  "name": "wikiform",
  "version": "1.2.1",
  "private": true,
  "main": "main/main.js",
  "author": "Saif Shahwan, Daniel Dausend, Hirokata Nakashima, Lukas VÃ¶gel",
  "description": "Electron + NextJS example project",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "start": "next dev",
    "electron": "electron-builder",
    "build": "rimraf dist && next build",
    "export": "next export",
    "lint": "next lint"
  },
  "dependencies": {
    "electron-serve": "^1.3.0",
    "json2md": "^2.0.1",
    "next": "^14.1.4",
    "node-html-markdown": "^1.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "react-quill": "^2.0.0",
    "turndown": "^7.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron-forge/maker-rpm": "^7.3.1",
    "@types/json2md": "^1.5.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "electron": "^29.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.smooth.wikiform",
    "productName": "wikiform",
    "extraMetadata": {
      "version": "1.2.3"
    },
    "files": [
      "main/**/*",
      "renderer/**/*",
      "out/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    }
  }
}
